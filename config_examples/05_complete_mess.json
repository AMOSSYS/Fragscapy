{
  "cmd": "/usr/bin/wget http://www.example.com",

  "nfrules": [
    {"host": "www.example.com", "port": 80},
    {"host": "www.example.com", "port": 443},
    {"port": 22, "input_chain": "False", "ipv4": "False", "qnum":18}
  ],

  "input": [
    {
      "mod_name": "echo",
      "mod_opts": "seq_str foo bar fuz"
